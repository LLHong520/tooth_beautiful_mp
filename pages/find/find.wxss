.container {
  padding: 0 32rpx;
}
.home_page {
  width: 100%;
}
.act-wrapper {
  width: 100%;
}
.act-list {
  width: 100%;
}
.act-item {
  border-bottom: 1px solid #f3f3f3;
  padding: 0 0 42rpx;
}
.act-item .text-wrapper {
  margin: 32rpx 0 20rpx;
  font-size: 30rpx;
  color: #2D2D2D;
  font-weight:600;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  -webkit-box-orient: vertical;
}
.act-item .img-wrapper {
  width: 100%;
  height: 386rpx;
}
.act-item .img-wrapper image {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  background: #f5f5f5;
}
